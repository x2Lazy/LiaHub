if not game:GetService("CoreGui") then
	print("playergui")
	LiaHub.Parent=plr.PlayerGui
else
	print("coregui")
	local s,e=pcall(function()
		LiaHub.Parent=game:GetService("CoreGui")
	end)

	if s then
		print("welcome to lia hub")
	else
		print(e)
		LiaHub.Parent=plr.PlayerGui
	end
end

